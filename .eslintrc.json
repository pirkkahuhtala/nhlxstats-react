{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "extends": [
        "airbnb",
        "prettier"
    ],
    "rules": {
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "comma-dangle": [
            "error",
            "never"
        ],
        "jsx-a11y/href-no-hash": "off",
        "jsx-a11y/img-has-alt": [
            0
        ],
        "no-confusing-arrow": [
            0
        ],
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "react/jsx-no-bind": [
            0
        ],
        "react/prop-types": [
            1
        ],
        "react/require-default-props": [
            0
        ]
    }
}